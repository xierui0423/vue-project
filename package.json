{
  "name": "vue-project",
  "version": "1.0.0",
  "description": "Vue project boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "build": "webpack",
    "start": "webpack-dev-server"
  },
  "keywords": [
    "vue",
    "vuex",
    "router",
    "webpack"
  ],
  "author": "Ray Xie",
  "license": "MIT",
  "browserslist": [
    "last 4 versions",
    "ie >= 9"
  ],
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.4.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "postcss-cssnext": "^2.10.0",
    "sass-loader": "^6.0.3",
    "url-loader": "^0.5.8",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "^2.2.6",
    "webpack-dev-server": "^2.4.4"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "vue": "^2.2.6",
    "vuex": "^2.3.1",
    "webpack": "^2.4.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "$": true
    },
    "extends": [
      "airbnb"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "new-cap": [
        0
      ],
      "linebreak-style": [
        2,
        "windows"
      ],
      "import/prefer-default-export": [
        0
      ],
      "no-confusing-arrow": [
        0
      ],
      "class-methods-use-this": [
        0
      ],
      "no-underscore-dangle": [
        0
      ],
      "no-console": [
        0
      ]
    }
  }
}
